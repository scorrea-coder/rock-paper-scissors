{
	"name": "game-of-drones",
	"version": "1.0.0",
	"description": "Rock , Paper , Scissors Back End",
	"main": "src/server.js",
	"scripts": {
		"start": "node dist/server.js",
		"build": "babel src --out-dir dist",
		"serve": "nodemon --exec babel-node src/server.js",
		"dev": "concurrently \"npm run serve\" \"npm start --prefix client\""
	},
	"keywords": [
		"Rock",
		"Paper",
		"Scissors",
		"Game"
	],
	"author": "Santiago Correa Espinosa",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.4.5",
		"concurrently": "^4.1.0",
		"core-js": "^3.1.3",
		"cors": "^2.8.5",
		"dotenv": "^8.0.0",
		"express": "^4.18.2",
		"mongoose": "^5.5.13",
		"nodemon": "^1.19.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/node": "^7.4.5",
		"@babel/plugin-transform-runtime": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"eslint": "^5.16.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-import": "^2.17.3",
		"redux-devtools": "^3.5.0"
	}
